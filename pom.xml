                <version>${demo.version}</version>
            </dependency>
            <dependency>
                <groupId>com.guonanjun.demo</groupId>
                <artifactId>demo-manager</artifactId>
                <version>${demo.version}</version>
            </dependency>
            <dependency>
                <groupId>com.guonanjun.demo</groupId>
